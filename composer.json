    "keywords": [
    	"laravel", 
    	"Survloop", 
    	"Don't Fallacy Me Bro",
    	"logical fallacy", 
    	"games", 
    	"multiplayer", 
    	"crowd-sourced", 
    	"critical thinking", 
    	"logical fallacies", 
    	"brain games"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Morgan Lesko",
            "homepage": "https://github.com/rockhopsoft/fallacy",
            "email": "rockhoppers@runbox.com",
            "role": "Developer"
        }
    ],
    "require": {
        "rockhopsoft/survloop": "*@dev",
        "rockhopsoft/fallacy-memes": "*@dev"
    },
    "autoload": {
        "psr-4": {
            "RockHopSoft\\Fallacy\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RockHopSoft\\Fallacy\\FallacyServiceProvider"
            ],
            "aliases": {
                "Fallacy": "RockHopSoft\\Fallacy\\FallacyFacade"
            }
        }
    }
